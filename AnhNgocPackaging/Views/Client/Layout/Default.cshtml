<!DOCTYPE html>

<html>
<head>
    <!-- ========== Meta Tags ========== -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="gramentheme">
    <meta name="description" content="Bookle - Book Store WooCommerce Html Template ">
    <!-- ======== Page title ============ -->
    <title>Bookle - Book Store WooCommerce Html Template</title>
    <!--<< Favcion >>-->
    <link rel="shortcut icon" href="~/images/favicon.png">
    <!--<< Bootstrap min.css >>-->
    <link rel="stylesheet" href="~/css/client/bootstrap.min.css" asp-append-version="true">
    <!--<< All Min Css >>-->
    <link rel="stylesheet" href="~/css/client/all.min.css" asp-append-version="true">
    <!--<< Animate.css >>-->
    <link rel="stylesheet" href="~/css/client/animate.css" asp-append-version="true">
    <!--<< Magnific Popup.css >>-->
    <link rel="stylesheet" href="~/css/client/magnific-popup.css" asp-append-version="true">
    <!--<< MeanMenu.css >>-->
    <link rel="stylesheet" href="~/css/client/meanmenu.css" asp-append-version="true">
    <!--<< Swiper Bundle.css >>-->
    <link rel="stylesheet" href="~/css/client/swiper-bundle.min.css" asp-append-version="true">
    <!--<< Nice Select.css >>-->
    <link rel="stylesheet" href="~/css/client/nice-select.css" asp-append-version="true">
    <!--<< Icomoon.css >>-->
    <link rel="stylesheet" href="~/css/client/icomoon.css" asp-append-version="true">
    <!--<< Main.css >>-->
    <link rel="stylesheet" href="~/css/client/main.css" asp-append-version="true">


</head>
<body>

    <!-- Cursor follower -->
    <div class="cursor-follower"></div>

    <!-- Preloader Start -->
    <div id="preloader" class="preloader">
        <div class="animation-preloader">
            <div class="spinner">
            </div>
            <div class="txt-loading">
                <span data-text-preloader="B" class="letters-loading">
                    B
                </span>
                <span data-text-preloader="O" class="letters-loading">
                    O
                </span>
                <span data-text-preloader="O" class="letters-loading">
                    O
                </span>
                <span data-text-preloader="K" class="letters-loading">
                    K
                </span>
                <span data-text-preloader="L" class="letters-loading">
                    L
                </span>
                <span data-text-preloader="E" class="letters-loading">
                    E
                </span>
            </div>
            <p class="text-center">Loading</p>
        </div>
        <div class="loader">
            <div class="row">
                <div class="col-3 loader-section section-left">
                    <div class="bg"></div>
                </div>
                <div class="col-3 loader-section section-left">
                    <div class="bg"></div>
                </div>
                <div class="col-3 loader-section section-right">
                    <div class="bg"></div>
                </div>
                <div class="col-3 loader-section section-right">
                    <div class="bg"></div>
                </div>
            </div>
        </div>
    </div>

    <!-- Back To Top Start -->
    <button id="back-top" class="back-to-top">
        <i class="fa-solid fa-chevron-up"></i>
    </button>
    @await Component.InvokeAsync("Offcanvas")
    @await Component.InvokeAsync("HeaderSticky")
    @await Component.InvokeAsync("HeaderMain")
    @RenderBody()
    @await Component.InvokeAsync("Footer")
    <script type="importmap"></script>

    <!--<< jquery.min.js >>-->
    @* <script src="~/lib/jquery/dist/jquery.min.js"></script> *@
    <script src="~/js/client/jquery-3.7.1.min.js"></script>
    <!--<< Viewport Js >>-->
    <script src="~/js/client/viewport.jquery.js"></script>
    <!--<< Bootstrap Js >>-->
    <script src="~/js/client/bootstrap.bundle.min.js"></script>
    <!--<< Nice Select Js >>-->
    <script src="~/js/client/jquery.nice-select.min.js"></script>
    <!--<< Waypoints Js >>-->
    <script src="~/js/client/jquery.waypoints.js"></script>
    <!--<< Counterup Js >>-->
    <script src="~/js/client/jquery.counterup.min.js"></script>
    <!--<< Swiper Slider Js >>-->
    <script src="~/js/client/swiper-bundle.min.js"></script>
    <!--<< MeanMenu Js >>-->
    <script src="~/js/client/jquery.meanmenu.min.js"></script>
    <!--<< Magnific Popup Js >>-->
    <script src="~/js/client/jquery.magnific-popup.min.js"></script>
    <!--<< Wow Animation Js >>-->
    <script src="~/js/client/wow.min.js"></script>
    <!-- Gsap -->
    <script src="~/js/client/gsap.min.js"></script>
    <!--<< Main.js >>-->
    <script src="~/js/client/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
